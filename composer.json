{
  "name": "john-ask-later/calc-fn",
  "description": "Implementation of test task",
  "minimum-stability": "dev",
  "homepage": "https://github.com/john-ask-later/test-task-calc-fn",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeniy N.",
      "email": "john.ask.later@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "dev-master"
  },
  "scripts": {
    "sniff": [
      "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "lint": [
      "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ]
  },
  "autoload": {
    "psr-4": {
      "JohnAskLater\\CalcFn\\": "src"
    },
    "files": [
      "src/calc_fn.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JohnAskLater\\CalcFn\\Tests\\": "tests"
    }
  }
}
